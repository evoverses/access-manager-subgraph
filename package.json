{
  "name": "access-manager-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --studio --version-label v1.0.2",
    "deploy:ethereum": "npm run deploy -- --network mainnet access-manager-mainnet",
    "deploy:sepolia": "npm run deploy -- --network sepolia access-manager-sepolia",
    "deploy:optimism": "npm run deploy -- --network optimism access-manager-optimism",
    "deploy:optimism-sepolia": "npm run deploy -- --network optimism-sepolia access-manager-op-sepolia",
    "deploy:bsc": "npm run deploy -- --network bsc access-manager-bsc",
    "deploy:bsc-chapel": "npm run deploy -- --network chapel access-manager-chapel",
    "deploy:polygon": "npm run deploy -- --network matic access-manager-matic",
    "deploy:polygon-amoy": "npm run deploy -- --network polygon-amoy access-manager-polygon-amoy",
    "deploy:fantom": "npm run deploy -- --network fantom access-manager-fantom",
    "deploy:arbitrum": "npm run deploy -- --network arbitrum-one access-manager-arbitrum-one",
    "deploy:arbitrum-sepolia": "npm run deploy -- --network arbitrum-sepolia access-manager-arb-sepolia",
    "deploy:avalanche": "npm run deploy -- --network avalanche access-manager-avalanche",
    "deploy:avalanche-fuji": "npm run deploy -- --network fuji access-manager-fuji",
    "deploy:base": "npm run deploy -- --network base access-manager-base",
    "deploy:base-sepolia": "npm run deploy -- --network base-sepolia access-manager-base-sepolia",
    "deploy:zkevm": "npm run deploy -- --network zkevm access-manager-zkevm",
    "deploy:blast": "npm run deploy -- --network blast-mainnet access-manager-blast-mainnet",
    "deploy:blast-testnet": "npm run deploy -- --network blast-testnet access-manager-blast-testnet",
    "create-local": "graph create --node http://localhost:8020/ access-manager",
    "remove-local": "graph remove --node http://localhost:8020/ access-manager",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 access-manager",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.78.0",
    "@graphprotocol/graph-ts": "0.35.1"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
